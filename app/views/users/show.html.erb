<h1><%= @user.name %></h1>
<p><%= @user.email %></p>
<br />
<p><%= link_to 'Edit', edit_user_registration_path %></p>
<% if user_signed_in? %>
	<% if current_user == @user %>
	<p><%= link_to 'Upload Picture', new_user_picture_path(@user) %></p>
	<p><%= link_to 'My Pictures', @user.id.to_s() +'/myPictures' %> </p>
	<% end %>
<% end %>
<p><%= link_to 'List of Users', users_path %></p>